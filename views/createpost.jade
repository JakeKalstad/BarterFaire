h2 Create Posting  
form#dropzone.dropzone(style="min-height: 150px;margin: 0;top: 0;left: 0;") 
div.formSpace
    ul
        li
            label Locations
            div.styled-select
                select(data-bind="options: Locations, value: Location, optionsText: 'Name', optionsCaption: 'Choose a Location...'")
        li
            label Categories
            div.styled-select
                select(data-bind="options: Categories, value: Category, optionsText: 'Name', optionsCaption: 'Choose a Category...'")
        li
            label Title
            input(type="text", data-bind="value:Title")
        li
            label Body
            textarea(data-bind="value: Body", style="height:150px; width:950px")
    input.JKButton(type="button", data-bind="click: Submit", value="Create Posting") 
